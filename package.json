{
  "name": "tyler-dev-portfolio",
  "private": true,
  "description": "A portfolio for Tyler using Gatsby",
  "version": "0.1.0",
  "author": "Tyler Nguyen <tyler.t.nguyen@uconn.edu>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -o",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "update": "npx npm-check-updates -u",
    "deploy:github": "gatsby clean && gatsby build && node ./scripts/deploy-github",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prepare": "husky install"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "gatsby": "^4.10.0",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-manifest": "^4.10.0",
    "gatsby-plugin-offline": "^5.10.0",
    "gatsby-plugin-react-helmet": "^5.10.0",
    "gatsby-plugin-sass": "^5.10.0",
    "gatsby-plugin-sharp": "^4.10.0",
    "gatsby-source-filesystem": "^4.10.0",
    "gatsby-transformer-sharp": "^4.10.0",
    "nanoid": "^3.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-github-btn": "^1.2.1",
    "react-helmet": "^6.1.0",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.8.6",
    "react-tilt": "^0.1.4",
    "react-typical": "^0.1.3",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
